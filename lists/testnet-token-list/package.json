{
  "name": "@koanprotocol/test-token-list",
  "version": "0.0.0",
  "description": "koan protocol test token list",
  "main": "build/koan-test.tokenlist.json",
  "scripts": {
    "test": "mocha",
    "build": "rimraf build && mkdir -p build && node internal/write.js > build/koan-testnet.tokenlist.json",
    "prepublishOnly": "pnpm test && pnpm run build",
    "sync": "node internal/sync.js",
    "syncDB": "prisma generate --data-proxy --schema=./internal/schema.prisma && ts-node ./internal/syncDB.ts",
    "volume": "ts-node ./internal/tokenVolumes.ts",
    "ci:version": "pnpm version minor",
    "ci:publish": "pnpm publish --access public --tag latest"
  },
  "files": [
    "build/build/koan-test.tokenlist.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koan-protocol/assets.git",
    "directory": "lists/token-lists/default-token-list"
  },
  "keywords": [
    "Koan",
    "Koanprotocol",
    "testnet",
    "token",
    "list"
  ],
