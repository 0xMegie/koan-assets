{
  "name": "@koan/tokenlistdb",
  "version": "0.0.1",
  "description": "Koan Protocol Token Db",
  "keywords": [
    "Koanprotocol",
    "Token db"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/koan-protocol/token-list.git",
    "directory": "packages/db"
  },
  "license": "MIT",
  "author": "Sunday Ibrahim <whizibrahim@gmail.com>",
  "type": "module",
  "main": "dist/node.js",
  "source": "src/node.ts",
  "types": "dist/node.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    },
    "./edge": {
      "types": "./dist/edge.d.ts",
      "default": "./dist/edge.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build",
    "pull": "prisma db pull",
    "preinstall": "prisma generate",
    "push": "prisma db push --skip-generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "@prisma/adapter-neon": "^5.13.0",
    "@prisma/client": "^5.13.0",
    "@prisma/extension-accelerate": "^1.0.0",
    "prisma": "^5.13.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20",
    "@types/ws": "^8.5.10",
    "dotenv": "16.3.1",
    "typescript": "5.2.2"
  }
}
